    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "sinon-spy-react": "2.0.0",
    "style-loader": "0.13.1",
    "wallaby-webpack": "0.0.22",
    "webpack": "1.12.12",
    "webpack-config-loader": "0.1.0",
