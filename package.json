{
  "name": "adcentre-invoice-payment-ui",
  "version": "0.0.0",
  "description": "UI for paying AdCentre invoices.",
  "scripts": {
    "start-message": "babel-node scripts/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node scripts/srcServer.js",
    "open:dist": "babel-node scripts/distServer.js",
    "lint": "esw webpack.config.* app scripts",
    "lint:watch": "npm run lint -- --watch --quiet",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node scripts/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run test",
    "build": "babel-node scripts/build.js && npm run open:dist",
    "test": "mocha scripts/testSetup.js \"app/**/*.test.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root app --report html node_modules/mocha/bin/_mocha -- --require ./scripts/testSetup.js \"app/**/*.test.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "aphrodite": "0.3.1",
    "autoprefixer": "6.3.7",
    "axios": "0.12.0",
    "babel-polyfill": "6.9.1",
    "babel-runtime": "6.11.6",
    "base-64": "0.1.0",
    "css-reset": "git+ssh://git@github.com/SEEK-Jobs/css-reset",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "object-assign-deep": "0.0.4",
    "query-string": "4.2.2",
    "raygun4js": "2.3.3",
    "react": "15.1.0",
    "react-addons-css-transition-group": "15.1.0",
    "react-dom": "15.1.0",
    "react-number-format": "0.1.1",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-spin": "0.6.2",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
