{
  "predicates": [{
      "and": [{
          "equals": {
            "method": "GET"
          }
        },
        {
          "matches": {
            "path": "^/card/valid_token",
            "query": {
              "country": "Australia"
            }
          }
        }
      ]
    }
  ],
  "responses": [{
      "is": {
        "statusCode": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": "<% include responses/valid_token.json %>"
      }
    }
  ]
}
